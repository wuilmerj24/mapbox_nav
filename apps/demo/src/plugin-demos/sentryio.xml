<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" class="page">
    <Page.actionBar>
        <ActionBar title="sentryio" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>
    <StackLayout class="p-20">
        <ScrollView class="h-full">
          <StackLayout>
            <Button text="init" tap="{{ init }}" class="btn btn-primary"/>
            <Button text="captureException" tap="{{ captureException }}" class="btn btn-primary"/>
            <Button text="captureMessage" tap="{{ captureMessage }}" class="btn btn-primary"/>

          </StackLayout>
        </ScrollView>
    </StackLayout>
</Page>